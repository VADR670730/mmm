<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Registration form -->
        <template id="registration_attendee_details_more_fields_remove_ticket_type" inherit_id="website_event_registration_info.registration_attendee_details_more_fields">
            <xpath expr="//t[@t-foreach='tickets']//h4" position="replace">
                <h4 class="page-header mt16">
                    <strong>
                        <t t-esc="ticket['name']"/>
                    </strong>
                </h4>
            </xpath>
        </template>
    </data>
</odoo>
